{
    "manifest_version": 3,
    "version": "1.0",
    "name": "SFL Timer",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [{ "matches": ["<all_urls>"], "js": ["content.js"] }],
    "background":{
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "alarms"
    ]
    
}